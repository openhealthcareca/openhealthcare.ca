{{ template "discussions/nav" . }}
<div class="row topic">
	<div class="small-12 columns topic-body">

		<table class="topics">
			<tr>
				<th>#</th>
				<th>vote</th>
				<th>topic</th>
				<th>share</th>
			</tr>

		{{ range $i := .TestRange }}
