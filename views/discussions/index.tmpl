				</td>
				<td>
					<ul class="nav">
						<li><a href="#"><i class="fa fa-lg fa-facebook-square"></i></a></li>
						<li><a href="#"><i class="fa fa-lg fa-twitter-square"></i></a></li>
					</ul>
				</td>
			</tr>
		{{end}}

		</table>
		
		<div class="topic-content">


				</div>
	</div>
</div>
